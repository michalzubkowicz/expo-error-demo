{
  "expo": {
    "name": "mz-system",
    "description": "mz mobile app",
    "slug": "mz-system",
    "privacy": "unlisted",
    "platforms": [
      "android"
    ],
    "packagerOpts": {
      "sourceExts": ["idb"]
    },
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "backgroundColor": "#58A74F",
      "hidden": true
    },
    "splash": {
      "image": "./assets/images/splash_bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#58A74F"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "scheme": "naturesystem",
    "android": {
      "versionCode": 9,
      "package": "com.mz.mz",
      "googleServicesFile": "./google-services-dev.json",
      "config": {
        "googleSignIn": {
          "certificateHash": "3F:1F:FA:4F:5B:16:A9:C8:C5:17:B1:AA:EE:08:44:44:1F:00:6D:0E"
        }
      }
    }
  }
}
